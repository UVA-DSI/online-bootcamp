#Create empty vector
l <- c()
#Create loop with true condition
listUnfinished <- TRUE
while(listUnfinished){
  #Prompt User for input or to quit
  item <- readline(prompt = "What would you like to add to the list? Press 'q' to quit: ")
  if (item == 'q'){
    listUnfinished = FALSE
  } else {
  #Add input to list
    l <- c(l, item)
  }
}
#Print list when user is done
print(l)